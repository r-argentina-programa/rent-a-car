{
  "name": "rent-a-car",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start:api": "nest start --watch",
    "start": "node src/app.js",
    "schema:migrate": "npx sequelize-cli db:migrate",
