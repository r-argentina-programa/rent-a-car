{
  "name": "rent-a-car",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop --config ormconfig.migrations.ts",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync --config ormconfig.migrations.ts",
    "migration:run": "ts-node .\\\\node_modules\\\\typeorm\\\\cli.js migration:run --config ormconfig.migrations.ts",
    "migration:revert": "ts-node .\\\\node_modules\\\\typeorm\\\\cli.js migration:revert --config ormconfig.migrations.ts",
    "fixtures:run": "fixtures ./data/fixtures --config ormconfig.migrations.ts --debug --require=tsconfig-paths/register --require=ts-node/register",
    "dev": "nest start --watch",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "start": "nest start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpaternostro/rent-a-car.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mpaternostro/rent-a-car/issues"
  },
  "homepage": "https://github.com/mpaternostro/rent-a-car#readme",
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/testing": "^7.6.14",
    "@types/bcrypt": "^3.0.0",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.21",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typeorm-fixtures-cli": "^1.9.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@casl/ability": "^5.2.2",
    "@nestjs/common": "^7.6.14",
    "@nestjs/core": "^7.6.14",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.14",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "express": "^4.17.1",
    "faker": "^5.4.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.6",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/api",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../../coverage",
    "testEnvironment": "node"
  }
}
